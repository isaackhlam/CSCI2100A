#include <stdio.h>

int main(void){
    int n;
    scanf("%d", &n);
    while(n--){
        int m,max=0;
        scanf(" %d", &m);
        int a[m];
        while(m--){
            scanf(" %d",&a[m]);
            if(a[m]>max && !(a[m]%2)) max=a[m];
        }
        printf("%d\n",max);
    } 
    return 0;
}
